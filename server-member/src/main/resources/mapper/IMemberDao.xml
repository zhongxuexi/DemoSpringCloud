<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jess.dao.IMemberDao">
    <select id="getAll" resultType="com.jess.entity.Member">
        SELECT
        a.id,
        a.name,
        a.sex,
        a.age,
        a.create_time,
        a.update_time,
        a.status
        FROM
        t_user a
        ORDER BY
        a.id DESC
    </select>

    <insert id="insertMember" parameterType="com.jess.entity.Member">
        insert into
        t_user (name,sex,age)
        values (#{name},#{sex},#{age})
    </insert>

</mapper>